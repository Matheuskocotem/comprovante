{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/send-location",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/styles.css",
      "dest": "/server.js"
    },
    {
      "src": "/image.png",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}

